<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingConstraints">

    <data>
        <variable
            name="alchol"
            type="com.model.alchol_detail.Alchol" />
    </data>


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/horizontal_280_guideLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="320dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/horizontal_380_guideLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="410dp" />

            <ImageView
                android:id="@+id/imageView_background"
                android:layout_width="match_parent"
                android:layout_height="340dp"
                android:background="@null"
                setting_icon_str="@{alchol.backgroundMedia.get(0).mediaResource.medium.src.toString()}"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.485" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="340dp"
                android:background="#66000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/AlcholDetail_selectedByMe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="19dp"
                android:background="@null"
                android:onClick="onClick"
                android:src="@mipmap/empty_heart"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/detail_alcholinfo"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_alcholinfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/alcholdetail_top_radius"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/horizontal_280_guideLine">

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/horizontal_inner_50_guideLine"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_begin="100dp" />

                <TextView
                    android:id="@+id/AlcholDetail_alcholName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="2sp"
                    android:textColor="#000000"
                    android:text="@{alchol.name.kr.toString()}"
                    android:textSize="@dimen/detail_alchol_Name_k"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/horizontal_inner_50_guideLine"
                    tools:text="렉 앨리 임페리얼" />

                <TextView
                    android:id="@+id/AlcholDetail_alcholOrigin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:lineSpacingExtra="2sp"
                    android:textColor="#b4b4b4"
                    android:textSize="@dimen/detail_alchol_Name_E"
                    android:textStyle="normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:text="@{alchol.brewery.get(0).location.toString()}"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="@id/AlcholDetail_alcholName"
                    app:layout_constraintTop_toBottomOf="@id/AlcholDetail_alcholName"
                    tools:text="한국 경기도 평택" />

                <com.willy.ratingbar.ScaleRatingBar
                    android:id="@+id/detail_topRatingBar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintStart_toStartOf="@id/AlcholDetail_alcholOrigin"
                    app:layout_constraintTop_toBottomOf="@id/AlcholDetail_alcholOrigin"
                    app:srb_clearRatingEnabled="true"
                    app:srb_clickable="false"
                    app:srb_drawableEmpty="@mipmap/large_star_empty"
                    app:srb_drawableFilled="@mipmap/large_star_full"
                    app:layout_constraintEnd_toStartOf="@id/detail_topRatingBar_score"
                    app:srb_isIndicator="false"
                    app:srb_minimumStars="0"
                    app:srb_numStars="5"
                    app:srb_rating="0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:srb_scrollable="false"
                    app:srb_starPadding="1.5dp"
                    app:srb_stepSize="0.5" />

                <TextView
                    android:id="@+id/detail_topRatingBar_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:lineSpacingExtra="2sp"
                    android:gravity="center_horizontal"
                    tools:text="4.7"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:text="@{alchol.review.score.toString()}"
                    app:layout_constraintTop_toTopOf="@id/detail_topRatingBar"
                    app:layout_constraintBottom_toBottomOf="@id/detail_topRatingBar"
                    app:layout_constraintStart_toEndOf="@id/detail_topRatingBar"
                    app:layout_constraintEnd_toStartOf="@id/detail_reviewImg_top"
                    android:layout_marginStart="5dp"
                    />

                <ImageView
                    android:id="@+id/detail_reviewImg_top"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/mini_talk"
                    app:layout_constraintTop_toTopOf="@id/detail_topRatingBar_score"
                    app:layout_constraintBottom_toBottomOf="@id/detail_topRatingBar_score"
                    app:layout_constraintStart_toEndOf="@id/detail_topRatingBar_score"
                    app:layout_constraintEnd_toStartOf="@id/detail_reviewCount_top"
                    android:layout_marginStart="20dp"/>

                <TextView
                    android:id="@+id/detail_reviewCount_top"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:textColor="#333333"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    tools:text="30"
                    android:lineSpacingExtra="4sp"
                    android:text="@{alchol.review.reviewCount.toString()}"
                    app:layout_constraintTop_toTopOf="@id/detail_reviewImg_top"
                    app:layout_constraintBottom_toBottomOf="@id/detail_reviewImg_top"
                    app:layout_constraintStart_toEndOf="@id/detail_reviewImg_top"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="7dp"
                    />

                <ImageView
                    android:id="@+id/imageView_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:background="@null"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_type"
                    app:layout_constraintEnd_toStartOf="@id/imageView_dosu"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/detail_topRatingBar" />

                <TextView
                    android:id="@+id/textView_alcholType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:gravity="center_horizontal"
                    tools:text="맥주"
                    android:text="@{alchol.class_.firstClass.name.toString()}"
                    android:lineSpacingExtra="2sp"
                    android:textSize="@dimen/detail_default_textSize"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    app:layout_constraintEnd_toEndOf="@id/imageView_type"
                    app:layout_constraintStart_toStartOf="@id/imageView_type"
                    app:layout_constraintTop_toBottomOf="@id/imageView_type" />

                <ImageView
                    android:id="@+id/imageView_dosu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@mipmap/ic_alcohol"
                    app:layout_constraintBottom_toBottomOf="@id/imageView_type"
                    app:layout_constraintEnd_toStartOf="@+id/imageView_star"
                    app:layout_constraintStart_toEndOf="@id/imageView_type"
                    app:layout_constraintTop_toTopOf="@id/imageView_type" />

                <TextView
                    android:id="@+id/textView_dosu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:gravity="center_horizontal"
                    tools:text="10%"
                    android:text="@{alchol.abv.toString()}"
                    android:lineSpacingExtra="2sp"
                    android:textSize="@dimen/detail_default_textSize"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    app:layout_constraintEnd_toEndOf="@id/imageView_dosu"
                    app:layout_constraintStart_toStartOf="@id/imageView_dosu"
                    app:layout_constraintTop_toBottomOf="@id/imageView_dosu" />

                <ImageView
                    android:id="@+id/imageView_star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@mipmap/ic_star"
                    app:layout_constraintBottom_toBottomOf="@id/imageView_dosu"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/imageView_dosu"
                    app:layout_constraintTop_toTopOf="@id/imageView_dosu" />

                <TextView
                    android:id="@+id/detail_icRatringScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    tools:text="4.6 "
                    android:text="@{alchol.review.score.toString()}"
                    android:textSize="@dimen/detail_default_textSize"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    app:layout_constraintEnd_toEndOf="@id/imageView_star"
                    app:layout_constraintStart_toStartOf="@id/imageView_star"
                    app:layout_constraintTop_toBottomOf="@id/imageView_star" />

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcholType"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_description"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_alcholinfo"
                >
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textView_alcholExplain"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:layout_marginStart="16dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류설명"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0.45dp"
                    android:background="@color/orange"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toEndOf="@id/textView_alcholExplain"
                    app:layout_constraintBottom_toBottomOf="@id/textView_alcholExplain"/>

                <at.blogc.android.views.ExpandableTextView
                    android:id="@+id/textView_alcholDescription"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="30dp"
                    android:lineSpacingExtra="8sp"
                    android:textColor="#333333"
                    android:textSize="@dimen/detail_default_textSize"
                    android:maxLines="5"
                    app:animation_duration="170"
                    android:ellipsize="end"
                    android:text="그랑 크뤼 전통에서 양조 된 장인의 페일 에일. 황금빛 비등과 부드러운 홉 향을 즐겨보세요. 고수풀과 낙원의 곡물이 매운 미각을 둥글게하여 부드럽게 녹아서 호피와 행복의 비단결로 마무리합니다! 어떤 계절이든 축하하세요! 즐겨!"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcholExplain" />

                <LinearLayout
                    android:id="@+id/detail_expandableButton"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginTop="10dp"
                    android:onClick="onClick"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcholDescription"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/detail_arrow"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@mipmap/down_errow"
                        />

                    <TextView
                        android:id="@+id/detail_expandableText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textStyle="normal"
                        android:textSize="12sp"
                        android:textColor="#7b7b7b"
                        android:lineSpacingExtra="5sp"
                        android:text="더보기"
                        android:gravity="center_vertical"
                        android:layout_marginStart="5dp"
                        />
                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_component"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_description"
                >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textView_alcholinfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:layout_marginStart="16dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류정보"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintTop_toTopOf="parent" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/alcholComponent_recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="20dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcholinfo"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_chart"
                android:layout_width="match_parent"
                android:background="@color/white"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_component"
                android:layout_height="wrap_content">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textView_evalutionScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="18dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="사용자 평가지표"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>

                <Space
                    android:id="@+id/detail_space_top"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_evalutionScore" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/radarChart"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginTop="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/detail_space_top" />

                <Space
                    android:id="@+id/detail_space"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/radarChart" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:layout_marginTop="8dp"
                android:id="@+id/detail_review"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_chart">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textView_juryuReview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:layout_marginStart="16dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류리뷰"
                    android:textColor="#000000"
                    android:textSize="@dimen/title_Size"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/alchol_reviewSumCountText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="1sp"
                    android:hint="0개"
                    android:text="@{alchol.review.reviewCount.toString()}"
                    android:textColor="@color/orange"
                    android:textSize="@dimen/title_Size"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/textView_juryuReview"
                    app:layout_constraintStart_toEndOf="@id/textView_juryuReview"
                    app:layout_constraintTop_toTopOf="@id/textView_juryuReview" />

                <TextView
                    android:id="@+id/alcholDetail_reviewTotalscore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="51dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="4sp"
                    android:hint="0"
                    android:text="@{alchol.review.score.toString()}"
                    android:textColor="#000000"
                    android:textSize="@dimen/detail_review_Score"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toStartOf="@id/alcholDetail_scoreSeekbar"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_juryuReview" />

                <com.willy.ratingbar.ScaleRatingBar
                    android:id="@+id/alcholDetail_reviewRatingbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="@+id/alcholDetail_reviewTotalscore"
                    app:layout_constraintStart_toStartOf="@id/alcholDetail_reviewTotalscore"
                    app:layout_constraintTop_toBottomOf="@id/alcholDetail_reviewTotalscore"
                    app:srb_clearRatingEnabled="true"
                    app:srb_clickable="false"
                    app:srb_drawableEmpty="@mipmap/large_star_empty"
                    app:srb_drawableFilled="@mipmap/large_star_full"
                    app:srb_isIndicator="false"
                    app:srb_minimumStars="0"
                    app:srb_numStars="5"
                    app:srb_rating="0"
                    app:srb_scrollable="false"
                    app:srb_starPadding="1.5dp"
                    app:srb_stepSize="0.5" />

                <include
                    android:id="@+id/alcholDetail_scoreSeekbar"
                    layout="@layout/alchol_score_seekbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="50dp"
                    app:layout_constraintBottom_toBottomOf="@id/alcholDetail_reviewTotalscore"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/alcholDetail_reviewTotalscore"
                    app:layout_constraintTop_toTopOf="@id/alcholDetail_reviewTotalscore" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView_reviewList"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/default_outline"
                    android:nestedScrollingEnabled="false"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/alcholDetail_scoreSeekbar" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/AlcholDetail_evaluateButton"
                style="?android:attr/borderlessButtonStyle"
                android:layout_width="match_parent"
                android:layout_height="@dimen/ok_button_size"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="26dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/evalution_button_shape"
                android:onClick="onClick"
                android:text="평가하기"
                android:textColor="#ffffff"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_review" />

            <ImageView
