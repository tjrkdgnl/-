<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingConstraints">

    <data>

        <variable
            name="alcohol"
            type="com.model.alcohol_detail.Alcohol" />
    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/horizontal_280_guideLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="320dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/horizontal_380_guideLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="410dp" />

            <ImageView
                android:id="@+id/imageView_background"
                setting_icon_str="@{alcohol.backgroundMedia.get(0).mediaResource.medium.src.toString()}"
                android:layout_width="match_parent"
                android:layout_height="340dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.485" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="340dp"
                android:background="#66000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_alcoholinfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/alcoholdetail_top_radius"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/horizontal_280_guideLine">

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/horizontal_inner_50_guideLine"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_begin="100dp" />

                <TextView
                    android:id="@+id/AlcoholDetail_alcoholName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    android:gravity="center"
                    android:lineSpacingExtra="2sp"
                    android:maxLines="1"
                    android:text="@{alcohol.name.kr.toString()}"
                    android:textColor="#000000"
                    android:textSize="@dimen/detail_alcohol_Name_k"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/horizontal_inner_50_guideLine"
                    tools:text="렉 앨리 임페리얼" />

                <TextView
                    android:id="@+id/AlcoholDetail_alcoholOrigin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="23dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:gravity="center"
                    android:lineSpacingExtra="2sp"
                    android:maxLines="1"
                    android:text="@{alcohol.brewery.get(0).name.toString()}"
                    android:textColor="#b4b4b4"
                    android:textSize="@dimen/detail_alcohol_Name_E"
                    android:textStyle="normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="@id/AlcoholDetail_alcoholName"
                    app:layout_constraintTop_toBottomOf="@id/AlcoholDetail_alcoholName"
                    tools:text="한국 경기도 평택" />

                <ImageView
                    android:id="@+id/AlcoholDetail_selectedByMe"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@null"
                    android:onClick="onClick"
                    android:src="@mipmap/empty_heart"
                    app:layout_constraintHorizontal_bias="0.15"
                    app:layout_constraintStart_toStartOf="@id/ic_alcohol_type"
                    app:layout_constraintEnd_toEndOf="@id/ic_alcohol_type"
                    app:layout_constraintTop_toBottomOf="@id/AlcoholDetail_alcoholOrigin" />

                <TextView
                    android:id="@+id/alcoholdetail_likeCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginBottom="3dp"
                    android:lineSpacingExtra="7sp"
                    android:text="0"
                    android:textColor="#444444"
                    android:textSize="15sp"
                    android:textStyle="normal"
                    app:layout_constraintBottom_toBottomOf="@id/AlcoholDetail_selectedByMe"
                    app:layout_constraintStart_toEndOf="@id/AlcoholDetail_selectedByMe"
                    app:layout_constraintTop_toTopOf="@id/AlcoholDetail_selectedByMe" />

                <ImageView
                    android:id="@+id/detail_reviewImg_top"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/middle_talk"
                    app:layout_constraintBottom_toBottomOf="@id/AlcoholDetail_selectedByMe"
                    app:layout_constraintStart_toStartOf="@id/ic_degree_type"
                    app:layout_constraintEnd_toEndOf="@id/ic_degree_type"
                    app:layout_constraintHorizontal_bias="0.15"
                    app:layout_constraintTop_toTopOf="@id/AlcoholDetail_selectedByMe" />

                <TextView
                    android:id="@+id/detail_reviewCount_top"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:text="0"
                    android:textColor="#333333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="@id/detail_reviewImg_top"
                    app:layout_constraintStart_toEndOf="@id/detail_reviewImg_top"
                    app:layout_constraintTop_toTopOf="@id/detail_reviewImg_top" />

                <ImageView
                    android:id="@+id/detail_eye_topImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/middle_eye"
                    app:layout_constraintBottom_toBottomOf="@id/detail_reviewImg_top"
                    app:layout_constraintStart_toStartOf="@id/ic_star_type"
                    app:layout_constraintEnd_toEndOf="@id/ic_star_type"
                    app:layout_constraintHorizontal_bias="0.15"
                    app:layout_constraintTop_toTopOf="@id/detail_reviewImg_top" />

                <TextView
                    android:id="@+id/detail_eyeCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:lineSpacingExtra="7sp"
                    android:text="0"
                    android:textColor="#444444"
                    android:textSize="15sp"
                    android:textStyle="normal"
                    app:layout_constraintBottom_toBottomOf="@id/detail_eye_topImg"
                    app:layout_constraintStart_toEndOf="@id/detail_eye_topImg"
                    app:layout_constraintTop_toTopOf="@id/detail_eye_topImg"
                    tools:text="0" />

                <ImageView
                    android:id="@+id/ic_alcohol_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:background="@null"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_type"
                    app:layout_constraintEnd_toStartOf="@id/ic_degree_type"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/AlcoholDetail_selectedByMe" />

                <TextView
                    android:id="@+id/textView_alcoholType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="@{alcohol.class_.firstClass.name.toString()}"
                    android:textSize="@dimen/detail_default_textSize"
                    app:layout_constraintEnd_toEndOf="@id/ic_alcohol_type"
                    app:layout_constraintStart_toStartOf="@id/ic_alcohol_type"
                    app:layout_constraintTop_toBottomOf="@id/ic_alcohol_type"
                    tools:text="맥주" />

                <ImageView
                    android:id="@+id/ic_degree_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@mipmap/ic_alcohol"
                    app:layout_constraintBottom_toBottomOf="@id/ic_alcohol_type"
                    app:layout_constraintEnd_toStartOf="@+id/ic_star_type"
                    app:layout_constraintStart_toEndOf="@id/ic_alcohol_type"
                    app:layout_constraintTop_toTopOf="@id/ic_alcohol_type" />

                <TextView
                    android:id="@+id/textView_dosu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="@{alcohol.abv}"
                    android:textSize="@dimen/detail_default_textSize"
                    app:layout_constraintEnd_toEndOf="@id/ic_degree_type"
                    app:layout_constraintStart_toStartOf="@id/ic_degree_type"
                    app:layout_constraintTop_toBottomOf="@id/ic_degree_type"
                    tools:text="0%" />

                <ImageView
                    android:id="@+id/ic_star_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@mipmap/ic_star"
                    app:layout_constraintBottom_toBottomOf="@id/ic_degree_type"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/ic_degree_type"
                    app:layout_constraintTop_toTopOf="@id/ic_degree_type" />

                <TextView
                    android:id="@+id/detail_icRatringScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_sb"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="0.0 "
                    android:textSize="@dimen/detail_default_textSize"
                    app:layout_constraintEnd_toEndOf="@id/ic_star_type"
                    app:layout_constraintStart_toStartOf="@id/ic_star_type"
                    app:layout_constraintTop_toBottomOf="@id/ic_star_type" />

                <Button
                    android:id="@+id/AlcoholDetail_evaluateButton"
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/ok_button_size"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="26dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/evalution_button_shape"
                    android:onClick="onClick"
                    android:text="평가하기"
                    android:textColor="#ffffff"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcoholType" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_description"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_alcoholinfo">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_alcoholExplain"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="18dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류설명"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0.45dp"
                    android:layout_marginStart="8dp"
                    android:background="@color/orange"
                    app:layout_constraintBottom_toBottomOf="@id/textView_alcoholExplain"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/textView_alcoholExplain" />

                <at.blogc.android.views.ExpandableTextView
                    android:id="@+id/textView_alcoholDescription"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="16dp"
                    android:ellipsize="end"
                    android:lineSpacingExtra="8sp"
                    android:maxLines="5"
                    android:text="그랑 크뤼 전통에서 양조 된 장인의 페일 에일. 황금빛 비등과 부드러운 홉 향을 즐겨보세요. 고수풀과 낙원의 곡물이 매운 미각을 둥글게하여 부드럽게 녹아서 호피와 행복의 비단결로 마무리합니다! 어떤 계절이든 축하하세요! 즐겨!"
                    android:textColor="#333333"
                    android:textSize="@dimen/detail_default_textSize"
                    app:animation_duration="170"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcoholExplain" />

                <LinearLayout
                    android:id="@+id/detail_expandableButton"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="20dp"
                    android:onClick="onClick"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcoholDescription">

                    <ImageView
                        android:id="@+id/detail_arrow"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@mipmap/down_errow" />

                    <TextView
                        android:id="@+id/detail_expandableText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="5dp"
                        android:gravity="center_vertical"
                        android:lineSpacingExtra="5sp"
                        android:text="더보기"
                        android:textColor="#7b7b7b"
                        android:textSize="12sp"
                        android:textStyle="normal" />
                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_component"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_description">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_alcoholinfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="18dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류정보"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0.45dp"
                    android:layout_marginStart="8dp"
                    android:background="@color/orange"
                    app:layout_constraintBottom_toBottomOf="@id/textView_alcoholinfo"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/textView_alcoholinfo" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/alcoholComponent_recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_alcoholinfo" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_chart"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_component">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_evalutionScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="18dp"
                    android:fontFamily="@font/apple_sd_gothic_neo_b"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="사용자 평가지표"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title_Size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0.45dp"
                    android:layout_marginStart="8dp"
                    android:background="@color/orange"
                    app:layout_constraintBottom_toBottomOf="@id/textView_evalutionScore"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/textView_evalutionScore" />

                <Space
                    android:id="@+id/detail_space_top"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_evalutionScore" />

                <com.github.mikephil.charting.charts.RadarChart
                    android:id="@+id/radarChart"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/detail_space_top" />

                <ImageView
                    android:id="@+id/user_indicator"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:src="@mipmap/user_indicator"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/detail_space_top" />

                <Space
                    android:id="@+id/detail_space"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/radarChart" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/detail_review"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_chart">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.45dp"
                    android:background="@color/light_grey2"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_juryuReview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="18dp"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="2sp"
                    android:text="주류리뷰"
                    android:textColor="#000000"
                    android:textSize="@dimen/title_Size"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/alcohol_reviewSumCountText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:gravity="center_horizontal"
                    android:hint="0개"
                    android:lineSpacingExtra="1sp"
                    android:textColor="@color/orange"
                    android:textSize="@dimen/title_Size"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/textView_juryuReview"
                    app:layout_constraintStart_toEndOf="@id/textView_juryuReview"
                    app:layout_constraintTop_toTopOf="@id/textView_juryuReview" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0.45dp"
                    android:layout_marginStart="8dp"
                    android:background="@color/orange"
                    app:layout_constraintBottom_toBottomOf="@id/alcohol_reviewSumCountText"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/alcohol_reviewSumCountText" />

                <TextView
                    android:id="@+id/alcoholDetail_reviewTotalscore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="51dp"
                    android:gravity="center_horizontal"
                    android:hint="0"
                    android:lineSpacingExtra="4sp"
                    android:textColor="#000000"
                    android:textSize="@dimen/detail_review_Score"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toStartOf="@id/alcoholDetail_scoreSeekbar"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textView_juryuReview" />

                <com.willy.ratingbar.ScaleRatingBar
                    android:id="@+id/alcoholDetail_reviewRatingbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="@+id/alcoholDetail_reviewTotalscore"
                    app:layout_constraintStart_toStartOf="@id/alcoholDetail_reviewTotalscore"
                    app:layout_constraintTop_toBottomOf="@id/alcoholDetail_reviewTotalscore"
                    app:srb_clearRatingEnabled="true"
                    app:srb_clickable="false"
                    app:srb_drawableEmpty="@mipmap/large_star_empty"
                    app:srb_drawableFilled="@mipmap/large_star_full"
                    app:srb_isIndicator="false"
                    app:srb_minimumStars="0"
                    app:srb_numStars="5"
                    app:srb_rating="0"
                    app:srb_scrollable="false"
                    app:srb_starPadding="1.5dp"
                    app:srb_stepSize="0.5" />

                <include
                    android:id="@+id/alcoholDetail_scoreSeekbar"
                    layout="@layout/alcohol_score_seekbar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="50dp"
                    app:layout_constraintBottom_toBottomOf="@id/alcoholDetail_reviewTotalscore"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/alcoholDetail_reviewTotalscore"
                    app:layout_constraintTop_toTopOf="@id/alcoholDetail_reviewTotalscore" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView_reviewList"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/default_outline"
                    android:nestedScrollingEnabled="false"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/alcoholDetail_scoreSeekbar" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="58dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/detail_review" />

            <ImageView
                setting_icon_str="@{alcohol.media.get(0).mediaResource.medium.src.toString()}"
                android:layout_width="@dimen/alcoholSize_width"
                android:layout_height="@dimen/alcoholSize_height"
                android:layout_marginTop="45dp"
                android:background="@null"
                android:scaleType="fitCenter"
                android:src="@mipmap/nuggi"
                app:layout_constraintBottom_toBottomOf="@id/horizontal_380_guideLine"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</layout>