<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingConstraints">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/score_5Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="5점"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />

        <SeekBar
            android:id="@+id/score_5Seekbar"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:max="5"
            android:maxHeight="12dp"
            android:minHeight="@dimen/detail_review_progress_height"
            android:progress="0"
            android:progressDrawable="@drawable/progress_custom"
            android:thumb="@null"
            android:progressTint="#FFC77B"
            android:thumbOffset="0dp"
            app:layout_constraintStart_toEndOf="@id/score_5Text"
            app:layout_constraintTop_toTopOf="@id/score_5Text"
            app:layout_constraintBottom_toBottomOf="@id/score_5Text"
            />

        <TextView
            android:id="@+id/score_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/score_5Seekbar"
            app:layout_constraintTop_toTopOf="@id/score_5Seekbar"
            app:layout_constraintBottom_toBottomOf="@id/score_5Seekbar"
            />

        <TextView
            android:id="@+id/score_4Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="4점"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@id/score_5Text"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="15dp"
            />

        <SeekBar
            android:id="@+id/score_4Seekbar"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:max="5"
            android:maxHeight="12dp"
            android:minHeight="@dimen/detail_review_progress_height"
            android:progress="0"
            android:progressDrawable="@drawable/progress_custom"
            android:thumb="@null"
            android:thumbOffset="0dp"
            android:progressTint="#FFD59C"
            app:layout_constraintTop_toTopOf="@id/score_4Text"
            app:layout_constraintBottom_toBottomOf="@id/score_4Text"
            app:layout_constraintStart_toEndOf="@id/score_4Text"
            />

        <TextView
            android:id="@+id/score_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/score_4Seekbar"
            app:layout_constraintTop_toTopOf="@id/score_4Seekbar"
            app:layout_constraintBottom_toBottomOf="@id/score_4Seekbar"
            />

        <TextView
            android:id="@+id/score_3Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="3점"
            android:textStyle="bold"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@id/score_4Seekbar"
            />

        <SeekBar
            android:id="@+id/score_3Seekbar"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:max="5"
            android:maxHeight="12dp"
            android:minHeight="@dimen/detail_review_progress_height"
            android:progress="0"
            android:progressDrawable="@drawable/progress_custom"
            android:thumb="@null"
            android:thumbOffset="0dp"
            android:progressTint="#FFDCAC "
            app:layout_constraintStart_toEndOf="@id/score_3Text"
            app:layout_constraintTop_toTopOf="@id/score_3Text"
            app:layout_constraintBottom_toBottomOf="@id/score_3Text"
            />

        <TextView
            android:id="@+id/score_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/score_3Seekbar"
            app:layout_constraintTop_toTopOf="@id/score_3Seekbar"
            app:layout_constraintBottom_toBottomOf="@id/score_3Seekbar"
            />

        <TextView
            android:id="@+id/score_2Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="2점"
            android:textStyle="bold"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@id/score_3Text"
            app:layout_constraintStart_toStartOf="parent"
            />

        <SeekBar
            android:id="@+id/score_2Seekbar"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:max="5"
            android:maxHeight="12dp"
            android:minHeight="@dimen/detail_review_progress_height"
            android:progress="0"
            android:progressDrawable="@drawable/progress_custom"
            android:thumb="@null"
            android:thumbOffset="0dp"
            android:progressTint="#FFEACD"
            app:layout_constraintStart_toEndOf="@id/score_2Text"
            app:layout_constraintTop_toTopOf="@id/score_2Text"
            app:layout_constraintBottom_toBottomOf="@id/score_2Text"
            />

        <TextView
            android:id="@+id/score_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/score_2Seekbar"
            app:layout_constraintTop_toTopOf="@id/score_2Seekbar"
            app:layout_constraintBottom_toBottomOf="@id/score_2Seekbar"
            />

        <TextView
            android:id="@+id/score_1Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="1점"
            android:layout_marginTop="15dp"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@id/score_2Text"
            app:layout_constraintStart_toStartOf="parent"
            />

        <SeekBar
            android:id="@+id/score_1Seekbar"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:max="5"
            android:maxHeight="12dp"
            android:minHeight="@dimen/detail_review_progress_height"
            android:progress="0"
            android:progressTint="#FFF4E4"
            android:progressDrawable="@drawable/progress_custom"
            android:thumb="@null"
            android:thumbOffset="0dp"
            app:layout_constraintStart_toEndOf="@id/score_1Text"
            app:layout_constraintTop_toTopOf="@id/score_1Text"
            app:layout_constraintBottom_toBottomOf="@id/score_1Text"
            />

        <TextView
            android:id="@+id/score_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/detail_default_textSize"
            android:textColor="#c7c7c7"
            android:lineSpacingExtra="2sp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/score_1Seekbar"
            app:layout_constraintTop_toTopOf="@id/score_1Seekbar"
            app:layout_constraintBottom_toBottomOf="@id/score_1Seekbar"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
